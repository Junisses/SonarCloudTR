    <Border Background="Transparent"
            CornerRadius="10">
        <Grid Loaded="Cargar">
            <!--#region Columnas y filas-->
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="20"/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition Width="20"/>
                <ColumnDefinition/>
                <ColumnDefinition Width="185"/>
                <ColumnDefinition Width="20"/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="20"/>
                <RowDefinition Height="30"/>
                <RowDefinition Height="10*"/>
                <RowDefinition Height="30"/>
                <RowDefinition Height="10*"/>
                <RowDefinition Height="30"/>
                <RowDefinition Height="10*"/>
                <RowDefinition Height="30"/>
                <RowDefinition Height="10*"/>
                <RowDefinition Height="30"/>
                <RowDefinition Height="10*"/>
                <RowDefinition Height="10*"/>
                <RowDefinition Height="30"/>
                <RowDefinition Height="30"/>
                <RowDefinition Height="10*"/>
                <RowDefinition Height="30"/>
                <RowDefinition Height="30"/>
                <RowDefinition Height="10*"/>
                <RowDefinition Height="20"/>

            </Grid.RowDefinitions>
            <!--#endregion-->
            <!--#region Titulo-->
            <!--#endregion-->
            <!--#region BOTONES-->
            <!--#region REGRESAR-->
            <Button Style="{DynamicResource ButtonStyleRegresar}" 
                    x:Name="BtnRegresar"
                    Content="Regresar"
                    Grid.Column="1"
                    Grid.Row="1"
                    Width="100"
                    Cursor="Hand"
                    Background="#751970"
                    Foreground="White"
                    Click="Regresar" 
                    Margin="42,0,43,0"/>
            <!--#endregion-->
            <!--#region CREAR-->
            <Button x:Name="BtnCrear"
                    Content="Ingresar CHECK IN"
                    Grid.Row="16"
                    Grid.Column="5"
                    Width="150"
                    Background="Green"
                    Foreground="White"
                    Style="{DynamicResource estiloCrear}"
                    Click="Crear"/>
            <!--#endregion-->

            <!--#region BOTON DESCARGAR-->
            <TextBlock Grid.Row="16"
                       Grid.Column="1"
                       Grid.ColumnSpan="2"
                       Text="Descargar Documento CHECK IN"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Center"/>
            <TextBlock Grid.Row="16"
                       Grid.Column="1"
                       Foreground="Gray"
                       FontSize="10"
                       Text="Se recomienda generar el documento"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Top" Margin="0,27,0,0" Grid.RowSpan="2"/>
            <TextBlock Grid.Row="17"
                       Grid.Column="1"
                       Grid.ColumnSpan="2"
                       Foreground="Gray"
                       FontSize="10"
                       Text="luego de realizar el pago completo de la reserva*"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Top" Margin="0,6,0,0"/>
            <Button Style="{DynamicResource AgregarTipoServicio}"
                    x:Name="BtnDescargar"
                    Grid.Column="2"
                    Grid.Row="15"
                    Cursor="Hand"
                    Width="50"
                    Height="50"
                    Background="Transparent"
                    BorderBrush="Transparent"
                    Click="Descargar" Margin="10,0,92,0" 
                    Grid.RowSpan="3">
                <Image Source="/Vistas/Images/pdf1.png" />
            </Button>
            <!--#endregion-->

            <!--#endregion-->
            <!--#region CONTENIDO-->

            <!--Texto y box Cliente-->
            <TextBlock Grid.Row="3"
                       Grid.Column="1"
                       Text="Cliente"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Center"/>
            <TextBox Grid.Row="3"
                     Grid.Column="2"
                     IsEnabled="false"
                     x:Name="tbCliente"
                     HorizontalContentAlignment="Center"
                     Style="{DynamicResource TextBoxStyle}" 
                     Background="#F3E5F5"/>

            <TextBox Grid.Row="0"
                     Grid.Column="2"
                     IsEnabled="false"
                     Visibility="Hidden"
                     x:Name="tbIDdepto"
                     HorizontalContentAlignment="Center"/>

            <!--Texto y box Cliente rut-->
            <TextBlock Grid.Row="3"
                       Grid.Column="4"
                       Text="DNI"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Center"/>
            <TextBox Grid.Row="3"
                     Grid.Column="5"
                     x:Name="tbRut"
                     IsEnabled="false"
                     HorizontalContentAlignment="Center"
                     Style="{DynamicResource TextBoxStyle}" 
                     Background="#F3E5F5"/>


            <!--Texto y box para Fecha desde-->
            <TextBlock Grid.Row="5"
                       Grid.Column="1"
                       Text="Desde"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Center"/>
            <DatePicker Style="{DynamicResource DatePickerStyle}" 
                        Grid.Row="5"
                        Grid.Column="2"
                        FontSize="15"
                        Foreground="Gray"
                        BorderBrush="#F3E5F5"
                        Background="#F3E5F5"
                        x:Name="cFechaDesde"
                        IsEnabled="false"
                        HorizontalContentAlignment="Center"/>

            <!--Texto y box para Fecha hasta-->
            <TextBlock Grid.Row="5"
                       Grid.Column="4"
                       Text="Hasta"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Center"/>
            <DatePicker Style="{DynamicResource DatePickerStyle}" 
                        Grid.Row="5"
                        Grid.Column="5"
                        FontSize="15"
                        Foreground="Gray"
                        BorderBrush="#F3E5F5"
                        Background="#F3E5F5"
                        x:Name="cFechaHasta"
                        IsEnabled="false"
                        HorizontalContentAlignment="Center"/>

            <!--Texto y box para estado reserva-->
            <TextBlock Grid.Row="7"
                       Grid.Column="1"
                       Text="Estado Reserva"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Center"/>
            <TextBox Grid.Row="7"
                     Grid.Column="2"
                     x:Name="cbEstadoReserva"
                     IsEnabled="false"
                     HorizontalContentAlignment="Center"
                     Style="{DynamicResource TextBoxStyle}" 
                     Background="#F3E5F5"/>

            <!--Texto y box para Fecha hasta-->
            <TextBlock Grid.Row="12"
                       Grid.Column="1"
                       Text="Fecha de ingreso"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Center"/>
            <DatePicker Style="{DynamicResource DatePickerStyle}" 
                        Grid.Row="12"
                        Grid.Column="2"
                        FontSize="15"
                        BorderBrush="#F3E5F5"
                        Background="#F3E5F5"
                        x:Name="cFechaIngreso"
                        HorizontalContentAlignment="Center">
                <DatePicker.Resources>
                    <Style TargetType="{x:Type DatePickerTextBox}">
                        <Setter Property="Control.Template">
                            <Setter.Value>
                                <ControlTemplate>
                                    <TextBox x:Name="PART_TextBox"
                                 Text="{Binding Path=SelectedDate, 
                                        StringFormat='yyyy-MM-dd HH:mmtt', 
                                        RelativeSource={RelativeSource AncestorType={x:Type DatePicker}}}" />
                                </ControlTemplate>
                            </Setter.Value>
                        </Setter>
                    </Style>
                </DatePicker.Resources>
            </DatePicker>
            <!--Texto y box para precio noche-->
            <TextBlock Grid.Row="9"
                       Grid.Column="1"
                       Text="Precio Noche"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Center"/>
            <TextBox Grid.Row="9"
                     Grid.Column="2"
                     x:Name="tbPrecioNoche"
                     HorizontalContentAlignment="Center"
                     Style="{DynamicResource TextBoxStyle}" 
                     Background="#F3E5F5"
                     IsEnabled="false"/>

            <!--Texto y box saldo-->
            <TextBlock Grid.Row="7"
                       Grid.Column="4"
                       Text="Saldo"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Center"/>
            <TextBox Grid.Row="7"
                     Grid.Column="5"
                     x:Name="tbSaldo"
                     HorizontalContentAlignment="Center"
                     Style="{DynamicResource TextBoxStyle}" 
                     Background="#F3E5F5"
                     IsEnabled="false"/>




            <TextBlock x:Name="Titulo"
                       Text="Reserva"
                       Grid.Column="2"
                       Grid.Row="0"
                       FontSize="30"
                       VerticalAlignment="Top"
                       HorizontalAlignment="Left" 
                       Height="41" 
                       Grid.ColumnSpan="3" 
                       Margin="68,3,0,0" 
                       Grid.RowSpan="2"/>

            <!--#region Generar Boleta para pagar saldo-->
            <TextBlock Grid.Row="12"
                       Grid.Column="4"
                       x:Name="txtGaleria"
                       Text="Pagar saldo"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Center"/>
            <Button Style="{DynamicResource ButtonStyleRegresar}" 
                    x:Name="BtnPago"
                    Content="Pagar"
                    Grid.Column="5"
                    Grid.Row="12"
                    Cursor="Hand"
                    Background="#A64B9F"
                    Foreground="White"
                    Click="Pagar"
                    CommandParameter="{Binding idReserva}"
                    />
            <!--#endregion-->
            <!--#region DataGrid-->
            <DataGrid Grid.Row="4"
                      Grid.Column="4"
                      x:Name="GridCheck"
                      AutoGenerateColumns="False"
                      Background="GhostWhite"
                      Foreground="#A64B9F"
                      FontSize="15"
                      CanUserAddRows="False"
                      CanUserResizeColumns="True"
                      Visibility="Hidden"
                      CanUserResizeRows="False"
                      CanUserSortColumns="False"
                      RowBackground="Transparent"
                      HeadersVisibility="Column"
                      GridLinesVisibility="Horizontal"
                      HorizontalGridLinesBrush="#A64B9F" 
                      Grid.ColumnSpan="2" Margin="0,0,0,35" Grid.RowSpan="10">
                <DataGrid.Columns>
                    <DataGridTextColumn Header="Artefacto"
                                        Width="*"
                                        Binding="{Binding descripcion}"/>
                    <DataGridTextColumn Header="Cantidad"
                                        Width="*"
                                        Binding="{Binding cantidad}"/>
                    <DataGridTextColumn Header="Valor Unitario"
                                        Width="*"
                                        Binding="{Binding valor}"/>
                    <DataGridTemplateColumn Header="Opciones"
                                            Width="100">
                    </DataGridTemplateColumn>
                </DataGrid.Columns>
            </DataGrid>
            <!--#endregion-->
            <!--#region FRAME GALERIA-->
            <Frame x:Name="FramePago"
                   Grid.Row="0"
                   Grid.RowSpan="19"
                   Grid.ColumnSpan="7"/>
            <!--#endregion-->
        </Grid>

    </Border>
</Page>
